{
  "name": "quisporro",
  "version": "1.0.0",
  "description": "A DraftJS plugin for supporting Markdown syntax shortcuts",
  "main": "lib/index.js",
  "scripts": {
    "clean": "node_modules/.bin/rimraf lib",
    "build": "npm run clean && npm run build:js",
    "build:js": "BABEL_DISABLE_CACHE=1 BABEL_ENV=production NODE_ENV=production node_modules/.bin/babel --out-dir='lib' --ignore='**/__test__/*' src",
    "build:demo": "NODE_ENV=production webpack --config demo/webpack.config.prod.js && rm -rf demo/public/css && cp -R demo/publicTemplate/* demo/public/",
    "prepublish": "npm run build",
    "start:dev": "node_modules/.bin/babel-node ./demo/server.js",
    "start": "npm run start:dev",
    "test": "mocha --compilers js:babel-core/register --require testHelper.js **/__test__/*.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/AlvertaHayes/quisporro.git"
  },
  "keywords": [
    "draftjs",
    "veritatis",
    "plugin",
    "markdown"
  ],
  "author": "BoydBahringer",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/AlvertaHayes/quisporro.git/issues"
  },
  "devDependencies": {
    "autoprefixer": "^6.5.3",
    "babel-cli": "^6.18.0",
    "babel-core": "^6.18.2",
    "babel-eslint": "^7.1.1",
    "babel-loader": "^6.2.8",
    "babel-polyfill": "^6.16.0",
    "babel-preset-es2015": "^6.18.0",
    "babel-preset-react": "^6.16.0",
    "babel-preset-react-hmre": "^1.1.1",
    "babel-preset-stage-0": "^6.16.0",
    "chai": "^3.5.0",
    "chai-enzyme": "^0.6.1",
    "css-loader": "^0.26.0",
    "css-modules-require-hook": "^4.0.5",
    "dirty-chai": "^1.2.2",
    "draft-js-plugins-editor": "^2.0.0-beta5",
    "enzyme": "^2.6.0",
    "eslint": "^3.11.1",
    "eslint-config-airbnb": "^13.0.0",
    "eslint-plugin-import": "^2.2.0",
    "eslint-plugin-jsx-a11y": "2.2.3",
    "eslint-plugin-mocha": "^4.7.0",
    "eslint-plugin-react": "^6.7.1",
    "express": "^4.14.0",
    "extract-text-webpack-plugin": "^1.0.1",
    "file-loader": "^0.9.0",
    "flow-bin": "^0.36.0",
    "history": "^2.0.0",
    "jsdom": "^9.8.3",
    "mocha": "^3.2.0",
    "postcss-loader": "^1.1.1",
    "react-addons-pure-render-mixin": "^15.4.1",
    "react-addons-test-utils": "^15.4.1",
    "rimraf": "^2.5.4",
    "static-site-generator-webpack-plugin": "^3.1.0",
    "style-loader": "^0.13.1",
    "url-loader": "^0.5.7",
    "webpack": "^1.13.3",
    "webpack-dev-middleware": "^1.8.4",
    "webpack-hot-middleware": "^2.13.2"
  },
  "dependencies": {
    "draft-js-prism": "^1.0.3",
    "decorate-component-with-props": "^1.0.2",
    "draft-js": "^0.9.1",
    "draft-js-checkable-list-item": "^2.0.2",
    "react": "^15.4.1",
    "react-dom": "^15.4.1"
  }
}